<% if @feeds.present? %>
  $('#add_form_modal [data-behavior~=subscribe_target]').html('<%= j render partial: "subscriptions/new", locals: {feeds: @feeds} %>');
<% else %>
  $('#add_form_modal [data-behavior~=feeds_search]').replaceWith('<%= j render partial: "feeds/search", locals: {query: params[:q], error: "No Feeds Found"} %>');
<% end %>
