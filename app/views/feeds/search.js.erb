<% if @basic_auth.present? %>
    if ($('[data-behavior~=feed_password_form]').length > 0) {
        $('.modal-purpose-subscribe .password-message').addClass('hide')
        $('.modal-purpose-subscribe .password-error').removeClass('hide')
        $('.modal-purpose-subscribe [data-behavior~=submit_add]').removeAttr('disabled');
    } else {
        $('.modal-purpose-subscribe [data-behavior~=subscribe_target]').html('<%= j render partial: "subscriptions/password", locals: {feed_url: @feed_url} %>');
        feedbin.showAuthField()
        window.history.replaceState({}, document.title, "/");
    }
<% elsif @feeds.present? %>
    <% present @feeds.first do |feed_presenter| %>
        <% if @feeds.first.twitter_user? %>
            $('.modal-purpose-subscribe [data-behavior~=feeds_search_favicon_target]').html('<%= j feed_presenter.favicon(@feeds.first) %>');
        <% else %>
            $('.modal-purpose-subscribe [data-behavior~=feeds_search_favicon_target]').html('<%= j favicon_with_host(@feeds.first.host) %>');
        <% end %>
        $('.modal-purpose-subscribe [data-behavior~=subscribe_target]').html('<%= j render partial: "subscriptions/new", locals: {feeds: @feeds, tag_editor: @tag_editor} %>');
        feedbin.showSubscribeResults()
        window.history.replaceState({}, document.title, "/");
        feedbin.updateFeedSearchMessage();
    <% end %>
<% else %>
    feedbin.showNotification('No feed found.', true);
<% end %>
