<div class="basement-toolbar feed-settings">
  <div class="toolbar-item">
      <%= form_for entry.feed, remote: true, url: feed_rename_path(entry.feed.id), autocomplete: 'off' do |f| %>
        <%= f.text_field :title, placeholder: entry.feed.title, namespace: entry.feed.id, spellcheck: false, data: {behavior: 'user_title', feed_id: entry.feed.id} %>
      <% end %>
  </div>
</div>
