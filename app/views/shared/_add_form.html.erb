<div class="modal fade form-only" tabindex="-1" role="dialog" id="add_form_modal">
</div>
<div class="hide" data-behavior="add_form_markup">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <%= render partial: 'feeds/search', locals: {query: nil, error: nil} %>
      <div class="modal-body add-content">
        <div data-behavior="subscribe_target" class="available-subscriptions"></div>
      </div>
      <div class="modal-footer">
        <button type="button" class="button button-bordered button-secondary" data-dismiss="modal">Cancel</button>
        <button type="button" class="button button-bordered button-primary" style="margin-left: 10px;" data-behavior="submit_add" disabled="disabled">Add</button>
      </div>
    </div>
  </div>
</div>
