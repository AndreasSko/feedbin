<div class="modal fade" tabindex="-1" role="dialog" id="view_link_wrap" data-behavior="view_link_wrap"></div>
<div class="hide" data-behavior="view_link_markup">
    <div class="modal-dialog modal-fluid">
        <div class="modal-content">
            <div class="modal-body">
                <%= form_tag(view_link_entries_path, remote: true, data: {behavior: 'view_link_form'}) do %>
                    <%= hidden_field_tag 'url', nil %>
                <% end %>
                <div data-behavior="markup_target"></div>
            </div>
        </div>
    </div>
</div>
